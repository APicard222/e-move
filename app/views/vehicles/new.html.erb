<h1>New Vehicle</h1>


<%= simple_form_for @vehicle do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :photo, as: :file %>
      <%= f.submit "Create Vehicle", class: "btn btn-primary btn-home" %>
<% end %>

<%= link_to('Back', vehicles_path) %>
