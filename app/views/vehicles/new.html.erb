<h1>New Vehicle</h1>

<%= form_for(@vehicle) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.submit 'Create Vehicle'%>
<% end %>
<%= link_to('Back', vehicles_path) %>
